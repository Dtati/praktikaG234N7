    "file = pd.read_csv('games.csv') # чтение файла\n",
    "file = file.rename(columns={'Name':'name', 'Platform' : 'platform', 'Year_of_Release' : 'year_of_release', 'Genre': 'genre', 'NA_sales' : 'na_sales', 'EU_sales' : 'eu_sales',\n",
    "                         'JP_sales' : 'jp_sales', 'Other_sales' : 'other_sales', 'Critic_Score' : 'critic_score', 'User_Score' :  'user_score', 'Rating' : 'rating'}) # изменение регистра файлов\n",
    "file"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 49,
   "metadata": {},
   "outputs": [],
   "source": [
    "median = file.year_of_release.median()\n",
    "file.year_of_release = file.year_of_release.fillna(median) # меняем на медиальное значение там где его не было.\n",
    "file.year_of_release = file.year_of_release.astype(int) # изменение типа файла чтобы вывод года был в формате целого"
   ]
